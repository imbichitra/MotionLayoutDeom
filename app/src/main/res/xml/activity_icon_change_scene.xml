<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="3000">
       <KeyFrameSet>
       </KeyFrameSet>
        <!--<OnClick />-->
        <OnSwipe />
        <OnClick motion:targetId="@+id/icon_6" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/view"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="70dp"
            android:layout_height="70dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="20dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent">
            <CustomAttribute
                motion:attributeName="padding"
                motion:customDimension="0dp"/>
        </Constraint>
        <Constraint
            android:id="@+id/icon_9"
            motion:layout_constraintLeft_toRightOf="@+id/icon_8"
            android:layout_width="130dp"
            android:layout_height="130dp"
            motion:layout_constraintTop_toBottomOf="@+id/icon_4"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/icon_8"
            motion:layout_constraintLeft_toRightOf="@+id/icon_7"
            android:layout_width="130dp"
            android:layout_height="130dp"
            motion:layout_constraintTop_toBottomOf="@+id/icon_4"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/icon_7"
            android:layout_width="130dp"
            android:layout_height="130dp"
            motion:layout_constraintTop_toBottomOf="@+id/icon_4"
            motion:layout_constraintLeft_toLeftOf="parent"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/icon_1"
            android:layout_width="65dp"
            android:layout_height="65dp"
            motion:layout_constraintLeft_toLeftOf="parent"
            motion:layout_constraintTop_toTopOf="parent">
            <CustomAttribute
                motion:attributeName="padding"
                motion:customDimension="0dp"/>
        </Constraint>
        <Constraint
            android:id="@+id/icon_2"
            motion:layout_constraintLeft_toRightOf="@+id/icon_1"
            android:layout_width="65dp"
            android:layout_height="65dp"
            motion:layout_constraintTop_toTopOf="parent" >
            <CustomAttribute
                motion:attributeName="padding"
                motion:customDimension="0dp"/>
        </Constraint>
        <Constraint
            android:id="@+id/icon_3"
            motion:layout_constraintLeft_toRightOf="@+id/icon_2"
            android:layout_width="70dp"
            android:layout_height="70dp"
            motion:layout_constraintTop_toTopOf="parent" >
            <CustomAttribute
                motion:attributeName="padding"
                motion:customDimension="0dp"/>
        </Constraint>
        <Constraint
            android:id="@+id/icon_4"
            android:layout_width="70dp"
            android:layout_height="70dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintLeft_toRightOf="@id/icon_3" >
            <CustomAttribute
                motion:attributeName="padding"
                motion:customDimension="0dp"/>
        </Constraint>
        <Constraint
            android:id="@+id/icon_5"
            motion:layout_constraintLeft_toRightOf="@+id/icon_4"
            android:layout_width="70dp"
            android:layout_height="70dp"
            motion:layout_constraintTop_toTopOf="parent" >
            <CustomAttribute
                motion:attributeName="padding"
                motion:customDimension="0dp"/>
        </Constraint>
        <Constraint
            android:id="@+id/icon_6"
            motion:layout_constraintLeft_toRightOf="@+id/icon_5"
            android:layout_width="70dp"
            android:layout_height="70dp"
            motion:layout_constraintTop_toTopOf="parent" >
            <CustomAttribute
                motion:attributeName="padding"
                motion:customDimension="0dp"/>
        </Constraint>
    </ConstraintSet>
</MotionScene>